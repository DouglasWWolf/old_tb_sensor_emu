           FIFO0=0x1008
           START=0x1010
            STOP=0x1014
CYCLES_PER_FRAME=0x1018
     IDLE_VALUES=0x101C

axireg $FIFO0 0x11111111
axireg $FIFO0 0x22222222
axireg $FIFO0 0x33333333
axireg $FIFO0 0x44444444

# Cycles per frame
axireg $CYCLES_PER_FRAME 256


# Idle values
axireg $IDLE_VALUES 0xFAFB

axireg $START 1


